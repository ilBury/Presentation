{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "moduleResolution": "Node",
        "skipLibCheck": true,
        "allowJs": false,
        "strictNullChecks": false,
        "strictPropertyInitialization": false,
        "rootDir": "./webapp",
        "outDir": "./dist",
        "typeRoots": [
            "./node_modules/@types",
            "./node_modules/@sapui5/types"
        ],
        "paths": {
            "app/products/*": [
                "./webapp/*"
            ]
        }
    },
    "include": [
        "./webapp/**/*"
    ]
}